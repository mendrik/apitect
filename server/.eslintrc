{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint","unused-imports","prettier", "effector"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended","prettier", "plugin:effector/recommended"],
  "root": true,
  "parserOptions": {

  },
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "@typescript-eslint/no-explicit-any": "error",
    "@typescript-eslint/no-non-null-assertion": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "import/extensions": "off",
    "import/no-cycle": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "no-useless-return": "warn",
    "arrow-body-style": ["error", "as-needed"],
    "sort-imports": "off",
    "no-console": ["off", { "allowAsStatement": true }],
    "no-debugger": "warn",
    "no-return-assign": "off",
    "no-underscore-dangle": "off",
    "no-void": ["off", { "allowAsStatement": true }],
    "prefer-arrow-callback": ["error", { "allowNamedFunctions": true }],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "unused-imports/no-unused-imports": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}