{
  "name": "apitect-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "",
    "database": "docker-compose up",
    "start": "tsnd --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "validate": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --fix src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "ansicolor": "1.1.100",
    "bcryptjs": "2.4.3",
    "date-fns": "2.28.0",
    "dotenv": "16.0.0",
    "effector": "22.2.0",
    "fastify": "3.27.1",
    "fastify-cors": "6.0.2",
    "fastify-websocket": "4.0.0",
    "i18next": "21.6.10",
    "jsonwebtoken": "8.5.1",
    "mongodb": "4.3.1",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.0.0",
    "sherlockjs": "1.4.2",
    "shrink-json": "0.1.7",
    "uuid": "8.3.2",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.1.1",
    "@types/bcryptjs": "2.4.2",
    "@types/history": "4.7.9",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "16.11.12",
    "@types/ramda": "0.27.60",
    "@types/uuid": "8.3.3",
    "@types/validator": "13.7.0",
    "@types/ws": "8.2.2",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-effector": "0.4.2",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.4",
    "prettier": "2.5.1",
    "source-map-explorer": "2.5.2",
    "ts-jest": "27.1.1",
    "ts-node-dev": "1.1.8",
    "tsc-watch": "4.5.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~shared/(.*)": "<rootDir>/../frontend/src/shared/$1"
    },
    "globals": {
      "tsconfig": "tsconfig.json"
    }
  }
}
