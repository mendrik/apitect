{
  "name": "apitect-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "",
    "database": "docker-compose up",
    "start": "tsnd --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "validate": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --fix src/**/*.{ts,tsx}",
    "upgrade": "npx npm-check-updates -u & npm i"
  },
  "dependencies": {
    "ansicolor": "1.1.100",
    "bcryptjs": "2.4.3",
    "date-fns": "2.28.0",
    "dotenv": "16.0.0",
    "effector": "22.3.0",
    "fastify": "3.29.0",
    "@fastify/cors": "7.0.0",
    "@fastify/websocket": "5.0.0",
    "i18next": "21.6.16",
    "jsonwebtoken": "8.5.1",
    "mongodb": "4.5.0",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.1.0",
    "sherlockjs": "1.4.2",
    "shrink-json": "0.2.0",
    "uuid": "8.3.2",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/history": "5.0.0",
    "@types/jest": "27.5.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.31",
    "@types/ramda": "0.28.12",
    "@types/uuid": "8.3.4",
    "@types/validator": "13.7.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-effector": "0.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "npm-check-updates": "^12.5.11",
    "prettier": "2.6.2",
    "source-map-explorer": "2.5.2",
    "ts-jest": "28.0.1",
    "ts-node-dev": "2.0.0-0",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~shared/(.*)": "<rootDir>/../frontend/src/shared/$1"
    },
    "globals": {
      "tsconfig": "tsconfig.json"
    }
  }
}
